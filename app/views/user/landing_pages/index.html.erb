<div class="d-md-flex flex-wrap">
  <div class="container-fluid text-center page-list-container" id="display_landing_pages">
    <h1 class="display-4 mt-5 mb-5 text-white">Mes pages</h1>
    <div class="mt-5 d-md-flex flex-wrap justify-content-center text-center align-item-center mx-auto" id="landing_pages">
      <% current_user.landing_pages.each do |page| %>
        <%= render "landing_pages/landing_page", page: page %>
      <% end %>
    </div>
  </div>
  <div class="container-fluid p-3 my-4 mx-auto rounded page-form-container h-100" id="new_landing_page">
    <%= render "form", landing_page: @landing_page, url: user_landing_pages_path %>
  </div>
</div>
