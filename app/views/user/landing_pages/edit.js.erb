
function refreshForm(innerHTML) {
  const newLandingPageForm = document.getElementById('new_landing_page');
  newLandingPageForm.innerHTML = innerHTML;
}

$('html,body').animate({scrollTop: $("#display_landing_pages").offset().top}, 'slow');
refreshForm('<%= j render "form", landing_page: @landing_page, url: user_landing_page_path(@landing_page) %>');

uploadImages();
