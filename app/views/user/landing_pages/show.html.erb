<%#= @landing_page.display( with_back_button: current_user.present? )
                 .html_safe %>

<%= @landing_page.html.html_safe %>
<%= @landing_page.css.strip %>
<script>
  const myStyle = document.createElement("style");
  const coco = <%= @landing_page.css.strip %>;
  myStyle.innerText = coco.trim();
  document.head.append(myStyle);
</script>

<% @landing_page.images.each do |image| %>
  <%= image_tag image %>
<% end %>


