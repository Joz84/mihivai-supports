
<div class="d-md-flex flex-wrap flex-column">
  <div class="container-fluid px-5 py-5" style="background:#74C7BE;color:white !important;" id="new_landing_page">
    <%= render "form", project: @project, url: user_projects_path %>
  </div>
  
  <div class="container-fluid text-center" style="overflow:auto;display:block;min-height:74vh" id="display_landing_pages">
    <h1 class="display-4 mt-5 mb-5">Mes projets</h1>
    <div class="mt-5 d-md-flex flex-wrap justify-content-center text-center align-item-center mx-auto">
      <% current_user.projects.each do |project| %>
        <div class="d-flex justify-content-center flex-column">
          <%= render "projects/project", object: project.metas, url:  edit_user_project_path(project) %>
          <div class="d-flex flex-wrap justify-content-center align-item-center">
            <% project.users.each do |user| %>
              <div class="mt-2 mb-3 mx-3">
                <%= user.first_name %>
              </div>
            <% end %>
          </div>
        </div>
      <% end %>
    </div>
  </div>


</div>
